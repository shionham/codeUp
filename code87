package codeUp;

import java.util.Scanner;

public class code87 {
    public static void main(String[] args) {
        // 1,2,3 을 순서대로 계속 더해갈 때 그 합이 입력한 정수보다 작을 동안만 더하는 프로그램

        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();

        int sum = 0;
        for (int i = 0; i <= n; i++) {
            sum += i;

            if (i % 3 != 0) {
                System.out.print(i + " ");
            }
            sc.close();
        }
    }
    }
