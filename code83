package codeUp;

public class code83 {
    public static void main(String[] args) {
        // 369 369

        for (int i = 1; i <= 100; i++) { // 1부터 100까지 반복
                if ( i % 3 == 0) {
                    System.out.println(i +" 박수");
                } else {
                    System.out.println(i);
                }
            }

    }
}
